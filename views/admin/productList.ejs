            <section class="content-main">
                <div class="content-header">
                    <div>
                        <h2 class="content-title card-title">Products List</h2>
                    </div>
                    <div>
                        <a href="/admin/productAddPage" class="btn btn-primary btn-sm rounded">Create new</a>
                    </div>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                      <table class="table table-hover text-center">
                        <thead>
                          <tr>
                            <th>Product</th>
                            <th>Stock</th>
                            <th>Actions</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% if (product && product.length > 0) { for (let i = 0; i <
                          product.length; i++) { %>
                          <tr>
                            <td width="25%">
                              
                                <div class="left">
                                    <img src="/images/<%= product[i].image %>" class="img-sm img-thumbnail" alt="Item" />
                                </div>
                                <div class="info pl-3">
                                  <h6 class="mb-0 title">
                                    <%= product[i].name %>
                                  </h6>
                                </div>
                        
                            </td>
                            <td>
                              <span class="badge rounded-pill alert-success"
                                ><%= product[i].stock %></span
                              >
                            </td>
                            <td>
                              <a
                                href="/admin/productEditPage?id=<%= product[i]._id %>"
                                class="btn btn-sm btn-danger rounded font-sm mt-15"
                                >Edit</a
                              >
                              <a
                                href="/admin/productDelete?id=<%= product[i]._id %>"
                                class="btn btn-sm btn-brand rounded font-sm mt-15"
                                >Delete</a
                              >
                            </td>
                           
                           
                          </tr>
                          <% } } else { %>
                          <tr>
                            <td colspan="5">Products Not Found</td>
                          </tr>
                          <% } %>
                        </tbody>
                      </table>
                    </div>
                  </div>
